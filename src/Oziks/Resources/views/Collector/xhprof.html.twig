{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <span>
            <img width="13" height="28" alt="Memory Usage" src="data:image/png;base64,">
            <span>XHProf</span>
        </span>
    {% endset %}
    {% set text %}
        <div class="sf-toolbar-info-piece">
            <b>Current report</b>
            {% if collector.report.runId != "" %}
            <span><a href="{{ collector.report.report }}" target="_blank">{{ collector.report.runId }}</a></span>
            {% else %}
            <span>none</span>
            {% endif %}
        </div>
        {% if collector.reports|length %}
        <div class="sf-toolbar-info-piece">
            <b>Last reports</b>
            <ul onchange="document.href=http://google.fr">
            {% for report in collector.reports %}
                <li><a href="{{ report.report }}" target="_blank">{{ report.runId }}</a></li>
            {% endfor %}
            </ul>
        </div>
        {% endif %}
    {% endset %}
    {% include '@WebProfiler/Profiler/toolbar_item.html.twig' with { 'link': false } %}
{% endblock %}
